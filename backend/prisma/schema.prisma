generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model user {
  id              String   @id @default(uuid())
  date_created    DateTime @default(now())
  email           String   @unique
  hashed_password String
}
